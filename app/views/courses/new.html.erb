<%= form_for @course do |form|  %>

	<%= form.hidden_field :user_id %>

	<%= form.label :name %>
	<%= form.text_field :name %>
	<br>
	<%= form.label :description %>
	<%= form.text_area :description %>
	<br>
	<%= form.label :quantity %>
	<%= form.select :quantity, (0..10)%>
	<br>
	<%= form.label :date %>
	<%= form.date_select :Date, :start_year => Date.today.year %>	
	<br>
	<%= form.label :price %>
	<%= form.text_field :price %>
	<br>
	<%= form.submit %>
<% end %>